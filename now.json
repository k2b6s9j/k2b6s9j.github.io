{
  "version": 2,
  "routes": [
    {
      "src": "/contact",
      "status": 301,
      "headers": { "Location": "/about#contact" }
    },
    {
      "src": "/projects/(.*)",
      "status": 301,
      "headers": {
        "Location": "/portfolio/$1"
      }
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/404" }
  ]
}
