{
  "name": "@keplersj/website",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/ node_modules/.prynne/ node_modules/.vite/",
    "dev": "node server.js",
    "bake": "node prebuild.js && git add .prynne/bake",
    "build": "PREBUILD=1 npm run build:server && echo '{\"type\":\"commonjs\"}' > ./dist/server/package.json && PREBUILD=1 npm run build:client && npm run build:generate",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/main.js --outDir dist/server",
    "build:generate": "node generate.js",
    "preview": "npm run build && npm run serve",
    "serve": "serve dist/client",
    "test": "jest",
    "prepare": "husky install"
  },
  "gatsby": {
    "plugins": [
      "gatsby-theme-kepler",
      {
        "resolve": "gatsby-plugin-google-analytics",
        "options": {
          "trackingId": "UA-123145115-1",
          "anonymize": true,
          "respectDNT": true
        }
      },
      {
        "resolve": "gatsby-plugin-verify-bing",
        "options": {
          "userIds": [
            "E5D802A305BE2DD99EB4484A4B1DD90E"
          ]
        }
      },
      {
        "resolve": "gatsby-plugin-verify-brave",
        "options": {
          "token": "245d4d7a81a3fba4f21a25d3af9e801782f597de0a46ffdc4afa1cc2717ffcbe",
          "domain": "keplersj.com"
        }
      }
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-starstuff",
      "preset-lint-retext-english"
    ]
  },
  "jest": {
    "projects": [
      {
        "preset": "jest-runner-prettier",
        "displayName": "lint:prettier",
        "testPathIgnorePatterns": [
          "/.cache/",
          "/.forestry/",
          "/content/",
          "/node_modules/",
          "/dist/",
          "/static/",
          "/package-lock.json",
          "/package.json"
        ]
      },
      {
        "preset": "jest-runner-remark",
        "displayName": "lint:remark"
      }
    ]
  },
  "dependencies": {
    "@a11y/skip-navigation": "0.0.10",
    "@atomico/hooks": "3.31.0",
    "@fortawesome/fontawesome-free": "6.0.0",
    "@vaadin/router": "1.7.4",
    "atomico": "1.46.0",
    "atomico-use-head": "1.2.0",
    "atomico-use-remark": "1.1.0",
    "highlight.js": "11.4.0",
    "js-yaml": "4.1.0",
    "modern-normalize": "1.1.0",
    "polished": "4.1.4",
    "rehype-autolink-headings": "6.1.1",
    "rehype-highlight": "5.0.2",
    "rehype-rewrite": "3.0.4",
    "rehype-shift-heading": "1.0.2",
    "rehype-slug": "5.0.1",
    "remark-frontmatter": "4.0.1",
    "remark-gfm": "3.0.1",
    "starstuff-components": "4.0.1",
    "starstuff-style": "1.1.19",
    "styled-custom-elements": "0.1.9",
    "webcomponent-markdown": "0.2.7"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.7.2",
    "@rollup/plugin-babel": "5.3.1",
    "@types/js-yaml": "4.0.5",
    "cpy": "8.1.2",
    "express": "4.17.3",
    "global-jsdom": "8.4.0",
    "globby": "12.2.0",
    "gray-matter": "4.0.3",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-runner-prettier": "0.3.7",
    "jest-runner-remark": "1.0.28",
    "jsdom": "19.0.0",
    "make-dir": "3.1.0",
    "ora": "6.1.0",
    "p-queue": "7.2.0",
    "prettier": "2.5.1",
    "rehype-minify-json-script": "3.0.0",
    "rehype-minify-whitespace": "5.0.0",
    "rehype-parse": "8.0.4",
    "rehype-remove-comments": "5.0.0",
    "rehype-stringify": "9.0.3",
    "remark-preset-lint-retext-english": "1.0.64",
    "remark-preset-lint-starstuff": "1.0.6",
    "rimraf": "3.0.2",
    "serve": "13.0.2",
    "sharp": "0.30.1",
    "typescript": "4.5.5",
    "unified": "10.1.1",
    "vite": "2.8.4",
    "vite-plugin-pwa": "0.11.13",
    "vite-plugin-rehype": "0.1.0",
    "whatwg-fetch": "3.6.2"
  }
}
