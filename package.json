{
  "name": "@keplersj/website",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "test": "jest"
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-starstuff"
    ]
  },
  "jest": {
    "projects": [
      {
        "preset": "jest-runner-prettier",
        "displayName": "lint:prettier",
        "testPathIgnorePatterns": [
          "/.cache/",
          "/.forestry/",
          "/coverage/",
          "/content/",
          "/node_modules/",
          "/public/",
          "/reports/",
          "/static/",
          "/package-lock.json",
          "/package.json"
        ]
      },
      {
        "preset": "jest-runner-remark",
        "displayName": "lint:remark"
      }
    ]
  },
  "dependencies": {
    "gatsby": "^2.18.8",
    "gatsby-plugin-google-analytics": "^2.1.29",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-verify-bing": "^1.1.4",
    "gatsby-theme-kepler": "^1.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "jest-runner-prettier": "^0.3.3",
    "jest-runner-remark": "^1.0.2",
    "prettier": "^1.19.1",
    "remark-preset-lint-starstuff": "^1.0.1"
  }
}
